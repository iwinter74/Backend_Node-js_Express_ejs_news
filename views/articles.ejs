
<%-include('./partials/head')%>
<%-include('./partials/navigation')%>

<body>
    <section>
        <% articles.forEach(elt => {%>
            <div>
<h1><%=elt.title%></h1>
<p><%=elt.publishedAt%></p>
<div class="container_img">
<img src="<%=elt.urlToImage%>" alt="">
</div>
<p><%=elt.content%></p>
<div class="container_more">
<span><%=elt.author%></span>
<a href="<%=elt.url%>" target="_blank">Click4More</a>
</div>
</div>
        <%})%>
    </section>
</body>
</html>